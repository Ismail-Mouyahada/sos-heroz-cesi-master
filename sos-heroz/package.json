{
  "name": "sos-heroz",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@faker-js/faker": "^7.6.0",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "^4.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.6",
    "webpack": "^5.82.0",
    "webpack-cli": "^4.10.0",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/attachment-lite": "^1.0.7",
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/bodyparser": "^8.1.7",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/drive": "^2.3.0",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/validator": "^12.4.2",
    "@adonisjs/view": "^6.2.0",
    "@mezielabs/flair": "^1.0.6",
    "adonis5-swagger": "^1.4.1",
    "alpinejs": "^3.12.1",
    "axios": "^1.4.0",
    "flowbite": "^1.6.5",
    "geolocation-utils": "^1.2.5",
    "leaflet": "^1.9.4",
    "luxon": "^3.3.0",
    "mapbox-gl": "^2.14.1",
    "mysql2": "^3.3.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
